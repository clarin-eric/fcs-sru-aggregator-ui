{
  "name": "fcs-sru-aggregator-ui",
  "private": true,
  "version": "0.0.0",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://git.saw-leipzig.de/text-plus/FCS/fcs-sru-aggregator-ui.git"
  },
  "scripts": {
    "dev": "vite",
    "dev:textplus": "VITE_CANONCIAL_URL='https://alpha-fcs.text-plus.org' VITE_API_URL='https://alpha-fcs.text-plus.org/rest/' vite",
    "dev:local": "VITE_CANONCIAL_URL='http://172.19.87.70:4019' VITE_API_URL='http://172.19.87.70:4019/rest/' vite",
    "preview": "vite preview",
    "build": "tsc -b && vite build",
    "build:bundle": "tsc -b && vite --mode bundle build",
    "lint": "eslint .",
    "deploy:saw-demo": "npm run deploy:saw-demo-build && npm run deploy:saw-demo-rsync",
    "deploy:saw-demo-build": "VITE_CANONCIAL_URL='https://alpha-fcs.text-plus.org' VITE_API_URL='https://alpha-fcs.text-plus.org/rest/' VITE_I18N_NS_CONTEXT_PREFIX=textplus VITE_LOCALE=en VITE_LOCALES='[\"en\", \"de\"]' VITE_DEPLOY_PATH='/aggregator' npm run build",
    "deploy:saw-demo-rsync": "rsync -av --delete dist/ saw-tppssi:public_html/aggregator",
    "build:antlr-ng": "npm run build:antlr-ng:fcs-ql && npm run build:antlr-ng:lexcql",
    "build:antlr-ng:fcs-ql": "antlr-ng -Dlanguage=TypeScript -v -l --exact-output-dir -o src/parsers/ src/vendor/fcs-ql/FCSLexer.g4 src/vendor/fcs-ql/FCSParser.g4",
    "build:antlr-ng:lexcql": "antlr-ng -Dlanguage=TypeScript -v -l --exact-output-dir -o src/parsers/ src/vendor/fcs-ql/LexLexer.g4 src/vendor/fcs-ql/LexParser.g4"
  },
  "dependencies": {
    "@nozbe/microfuzz": "^1.0.0",
    "@tanstack/react-query": "^5.83.0",
    "antlr4ng": "^3.0.16",
    "axios": "^1.11.0",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.13.1",
    "i18next": "^25.3.2",
    "i18next-resources-to-backend": "^1.2.1",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.6.1",
    "react-router": "^7.7.1",
    "react-slugify": "^4.0.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/node": "^24.1.0",
    "@types/picomatch": "^4.0.2",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.7.0",
    "antlr-ng": "^1.0.10",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "estree-walker": "^3.0.3",
    "globals": "^16.3.0",
    "magic-string": "^0.30.17",
    "picomatch": "^4.0.3",
    "rollup-plugin-visualizer": "^6.0.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.6",
    "vite-plugin-package-version": "^1.1.0"
  }
}
