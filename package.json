{
  "name": "fcs-sru-aggregator-ui",
  "private": true,
  "version": "1.0.0-alpha",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/clarin-eric/fcs-sru-aggregator-ui.git"
  },
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "tsc -b && vite build",
    "build:bundle": "tsc -b && vite --mode bundle build",
    "lint": "eslint .",
    "build:antlr-ng": "npm run build:antlr-ng:fcs-ql && npm run build:antlr-ng:lexcql",
    "build:antlr-ng:fcs-ql": "antlr-ng -Dlanguage=TypeScript -v -l --exact-output-dir -o src/parsers/ src/vendor/fcs-ql/FCSLexer.g4 src/vendor/fcs-ql/FCSParser.g4",
    "build:antlr-ng:lexcql": "antlr-ng -Dlanguage=TypeScript -v -l --exact-output-dir -o src/parsers/ src/vendor/fcs-ql/LexLexer.g4 src/vendor/fcs-ql/LexParser.g4"
  },
  "dependencies": {
    "@nozbe/microfuzz": "^1.0.0",
    "@tanstack/react-query": "^5.90.2",
    "antlr4ng": "^3.0.16",
    "axios": "^1.12.2",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "fcs-sru-aggregator-api-adapter-typescript": "github:clarin-eric/fcs-sru-aggregator-api-adapter-typescript#v1.2.0",
    "i18next": "^25.5.3",
    "i18next-resources-to-backend": "^1.2.1",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^16.0.0",
    "react-router": "^7.9.3",
    "react-slugify": "^4.0.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/node": "^24.7.0",
    "@types/picomatch": "^4.0.2",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^5.0.4",
    "antlr-ng": "^1.0.10",
    "eslint": "^9.37.0",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-react-refresh": "^0.4.23",
    "estree-walker": "^3.0.3",
    "globals": "^16.4.0",
    "magic-string": "^0.30.19",
    "picomatch": "^4.0.3",
    "rollup-plugin-visualizer": "^6.0.4",
    "simple-git": "^3.28.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.9"
  }
}
